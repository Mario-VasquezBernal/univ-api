generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/@prisma/client-profesores"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_PROFESORES")
}

model Profesor {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  nombre    String
  apellido  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("profesores")
}
